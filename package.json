    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intersection-observer": "9.8.2",
    "react-player": "^2.16.0",
    "react-use": "17.5.0",
    "schema-dts": "1.1.2",
    "tiny-invariant": "1.3.3",
    "typographic-base": "1.0.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.43.0",
    "@remix-run/dev": "2.8.1",
