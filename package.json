    "@shopify/cli": "3.58.0",
    "@shopify/cli-hydrogen": "^8.0.0",
    "@shopify/hydrogen": "^2024.4.0",
    "@shopify/remix-oxygen": "^2.0.4",
    "@weaverse/hydrogen": "3.1.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "2.1.0",
    "cross-env": "7.0.3",
